{
  "name": "skynet-monitor",
  "version": "1.0.0",
  "private": true,
  "description": "This plugin provide agent monitor and management for skynet, most other plugins depends on this",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-card": "^1.20.17",
    "@ant-design/pro-descriptions": "^1.11.16",
    "@ant-design/pro-form": "^1.73.0",
    "@ant-design/pro-layout": "^6.38.18",
    "@ant-design/pro-table": "^2.79.0",
    "bytes": "^3.1.2",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "randomstring": "^1.2.2",
    "react": "17.0.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "17.0.2",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/bytes": "^3.1.1",
    "@types/lodash": "^4.14.182",
    "@types/qs": "^6.9.7",
    "@types/randomstring": "^1.1.8",
    "@types/react": "^17.0.43",
    "@types/react-copy-to-clipboard": "^5.0.4",
    "@types/react-dom": "^17.0.14",
    "@umijs/plugin-qiankun": "^2.39.2",
    "@umijs/preset-react": "2.1.4",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  }
}
