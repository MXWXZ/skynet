{
  "name": "skynet",
  "version": "1.0.0",
  "private": true,
  "description": "Service integration and management system, optimized for home-lab use",
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.20.17",
    "@ant-design/pro-descriptions": "^1.11.16",
    "@ant-design/pro-form": "^1.73.0",
    "@ant-design/pro-layout": "^6.38.18",
    "@ant-design/pro-table": "^2.79.0",
    "antd": "^4.22.5",
    "lodash": "^4.17.21",
    "qs": "^6.11.0",
    "react": "17.x",
    "react-dom": "17.x",
    "react-recaptcha": "^2.3.10",
    "umi": "^3.5.30"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.182",
    "@types/qs": "^6.9.7",
    "@types/react": "17.x",
    "@types/react-dom": "17.x",
    "@types/react-recaptcha": "^2.3.4",
    "@umijs/plugin-qiankun": "^2.39.2",
    "@umijs/preset-react": "2.1.4",
    "@umijs/test": "^3.5.30",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "yorkie": "^2.0.0"
  }
}
